{
  "name": "records",
  "type": "module",
  "version": "1.0.0",
  "description": "A Vite config including esLint/Prettier/airbnbEslint",
  "scripts": {
    "frontend": "vite",
    "start": "concurrently -n 'backend,frontend' -c  \"red,blue\" \"nodemon  server/app\" \"npm run frontend\"",
    "build": "tsc && vite build",
    "preview": "vite preview 5000 --strictPort",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "npx stylelint \"**/*.scss\"",
    "stylelint:fix": "npx stylelint \"**/*.scss\" --fix",
    "prepare": "husky",
    "server": "nodemon server/app.js"
  },
  "license": "ISC",
  "devDependencies": {
    "@reduxjs/toolkit": "^2.3.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "bcrypt": "^5.1.1",
    "concurrently": "^9.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.14",
    "eslint-plugin-typescript-sort-keys": "^3.3.0",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "husky": "^9.1.6",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^15.2.10",
    "mongoose": "^8.7.3",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "sass": "^1.80.4",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-eslint2": "^5.0.1"
  },
  "lint-staged": {
    "*.{ts, tsx, js}": [
      "eslint"
    ],
    "*.{scss, css}": [
      "stylelint"
    ],
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0"
  }
}
